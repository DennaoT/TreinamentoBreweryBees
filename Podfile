# Uncomment the next line to define a global platform for your project
platform :ios, '17.4'
use_frameworks!

def setup_libs
  pod 'SwiftGen', '~> 6.0'
end

def external_libs
  pod 'SnapKit', '~> 5.6.0'
end

target 'TreinamentoBreweryBees' do
  # Comment the next line if you don't want to use dynamic frameworks
  setup_libs
  external_libs
  
end

